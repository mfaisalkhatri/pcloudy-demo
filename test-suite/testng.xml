<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="pCloudy Website tests" parallel="tests">
    <test name="pCloudy Website tests on windows 10 chrome browser version 109">
        <parameter name="id" value="1"/>
        <classes>
            <class name="io.github.mfaisalkhatri.tests.PcloudyWebsiteTests">
                <methods>
                    <include name="testHomePageText"/>
