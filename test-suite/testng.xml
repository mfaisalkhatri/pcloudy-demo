                </methods>
            </class>
        </classes>
    </test> <!-- Test -->
    <test name="pCloudy Website tests on safari browser on mac ventura">
        <parameter name="id" value="3"/>
        <classes>
            <class name="io.github.mfaisalkhatri.tests.PcloudyWebsiteTests">
                <methods>
                    <include name="testHomePageText"/>
                    <include name="testBlogPageText"/>
                </methods>
            </class>
        </classes>
    </test> <!-- Test -->
</suite> <!-- Suite -->

